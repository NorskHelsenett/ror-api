    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug ROR-Api",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/api/main.go",
            "env": "__ENV__"
        },
        {
            "name": "Debug ROR-Api tests",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/api/",
            "args": ["-v", "./test/...", "-cover"],
            "showLog": true
        }
    ]
}
